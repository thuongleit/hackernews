<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="studio.vifi.hknews">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="studio.vifi.hknews.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="studio.vifi.hknews.view.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="studio.vifi.hknews.view.story.StoryActivity"
            android:parentActivityName="studio.vifi.hknews.view.main.MainActivity" />
    </application>

</manifest>